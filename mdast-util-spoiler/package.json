{
  "name": "mdast-util-spoiler",
  "version": "1.0.0",
  "license": "MIT",
  "description": "Converts a `micromark` token stream into an `mdast` syntax tree.",
  "repository": {
    "type": "git",
    "url": "https://github.com/hlysine/remark-spoiler.git",
    "directory": "mdast-util-spoiler"
  },
  "keywords": [
    "markdown",
    "spoiler",
    "remark",
    "remark-plugin",
    "micromark",
    "micromark-plugin",
    "micromark-extension"
  ],
  "author": {
    "name": "Henry Lin",
    "url": "https://github.com/hlysine"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "clean": "npx del dist",
    "prepare": "npm run clean & npm run build",
    "pretest": "npm run clean & npm run build",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'test/**/*.ts'",
    "dev:fix-links": "npm link micromark-extension-spoiler"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^18.1.0",
    "@types/mdast": "^3.0.3",
    "@types/mocha": "^8.2.2",
    "@types/unist": "^2.0.3",
    "@wessberg/rollup-plugin-ts": "^1.3.14",
    "del-cli": "^3.0.1",
    "mdast-util-from-markdown": "^0.8.5",
    "mdast-util-to-markdown": "^0.6.5",
    "micromark": "^2.11.4",
    "mocha": "^8.3.2",
    "rollup": "^2.47.0",
    "ts-node": "^9.1.1",
    "unified": "^9.2.1",
    "unist-util-visit": "^3.0.1"
  },
  "dependencies": {
    "micromark-extension-spoiler": "file:../micromark-extension-spoiler"
  }
}
